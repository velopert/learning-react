{
  "name": "space-separated-tokens",
  "version": "1.1.2",
  "description": "Parse and stringify space-separated tokens",
  "license": "MIT",
  "keywords": [
    "dom",
    "html",
    "space",
    "separated",
    "tokens",
    "parse",
    "stringify"
  ],
  "repository": "wooorm/space-separated-tokens",
  "bugs": "https://github.com/wooorm/space-separated-tokens/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "trim": "0.0.1"
  },
  "devDependencies": {
    "browserify": "^16.0.0",
    "esmangle": "^1.0.1",
    "nyc": "^11.0.0",
    "prettier": "^1.12.0",
    "remark-cli": "^5.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "tape": "^4.4.0",
    "xo": "^0.20.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier --write '**/*.js' && xo --fix",
    "build-bundle": "browserify index.js --bare -s spaceSeparatedTokens > space-separated-tokens.js",
    "build-mangle": "esmangle space-separated-tokens.js > space-separated-tokens.min.js",
    "build": "npm run build-bundle && npm run build-mangle",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test.js",
    "test": "npm run format && npm run build && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "no-var": "off",
      "prefer-arrow-callback": "off"
    },
    "ignore": [
      "space-separated-tokens.js"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
