    "eslint": "^5.7.0",
    "eslint-config-dev": "^2.0.0",
    "postcss-tape": "^2.2.0",
    "pre-commit": "^1.2.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3"
